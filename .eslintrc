{
  "parser": "babel-eslint",
  "plugins": [
    "react-native",
    "react",
    "flowtype",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "indent": [
      "error",
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-indent": [
      2,
      "tab"
    ],
    "react/jsx-indent-props": [
      2,
      "tab"
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}